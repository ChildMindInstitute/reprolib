{
    "@context": [ "https://raw.githubusercontent.com/ReproNim/schema-standardization/master/contexts/generic.jsonld",
        "https://raw.githubusercontent.com/ReproNim/schema-standardization/master/activity-sets/voice-pilot/voice_pilot_context.jsonld"
    ],
    "@type": "https://raw.githubusercontent.com/ReproNim/schema-standardization/master/schemas/ActivitySet.jsonld",
    "@id": "voice_pilot_schema",
    "skos:prefLabel": "Voice Pilot Protocol Activity set",
    "skos:altLabel": "voice_pilot",
    "schema:description": "activity set for UW MIT Voice Pilot Protocol",
    "schema:schemaVersion": "0.0.1",
    "schema:version": "0.0.1",
    "schema:image": "https://raw.githubusercontent.com/ReproNim/schema-standardization/master/activity-sets/voice-pilot/mit_voice_pilot_applet_image.svg?sanitize=true",
    "variableMap": [
        {"variableName": "voice_screening_schema", "isAbout": "voice_screening_schema"},
        {"variableName": "nda_guid", "isAbout": "nda_guid"},
        {"variableName": "gad7_schema", "isAbout": "gad7_schema"},
        {"variableName": "bads_schema", "isAbout": "bads_schema"},
        {"variableName": "promis_schema", "isAbout": "promis_schema"},
        {"variableName": "voice_task_schema", "isAbout": "voice_task_schema"}
    ],
    "ui": {
        "order": [
            "voice_screening_schema",
            "nda_guid",
            "gad7_schema",
            "bads_schema",
            "promis_schema",
            "voice_task_schema"
        ],
        "shuffle": false,
        "activity_display_name": {
            "voice_screening_schema":  "Screening",
            "nda_guid":  "Demographics",
            "gad7_schema":  "General anxiety",
            "bads_schema":  "Behavioral",
            "promis_schema":  "PROMIS Measure",
            "voice_task_schema": "Voice Tasks"
        },
        "visibility": {
            "voice_screening_schema": true,
            "nda_guid": {
              "method": "POST",
              "url": "https://us-central1-dmriprep.cloudfunctions.net/mit_voice_pilot",
              "payload": ["voice_screening_schema"]
            },
            "gad7_schema": {
              "method": "POST",
              "url": "https://us-central1-dmriprep.cloudfunctions.net/mit_voice_pilot",
              "payload": ["voice_screening_schema"]
            },
            "bads_schema": {
              "method": "POST",
              "url": "https://us-central1-dmriprep.cloudfunctions.net/mit_voice_pilot",
              "payload": ["voice_screening_schema"]
            },
            "promis_schema": {
              "method": "POST",
              "url": "https://us-central1-dmriprep.cloudfunctions.net/mit_voice_pilot",
              "payload": ["voice_screening_schema"]
            },
            "general_health_schema": {
              "method": "POST",
              "url": "https://us-central1-dmriprep.cloudfunctions.net/mit_voice_pilot",
              "payload": ["voice_screening_schema"]
            },
            "voice_task_schema": {
              "method": "POST",
              "url": "https://us-central1-dmriprep.cloudfunctions.net/mit_voice_pilot",
              "payload": ["voice_screening_schema"]
            }
        }
    }
}
